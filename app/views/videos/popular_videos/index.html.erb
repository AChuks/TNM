<h1>Listing popular_videos</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Mdata</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @popular_videos.each do |popular_video| %>
      <tr>
        <td><%= popular_video.url %></td>
        <td><%= popular_video.mdata %></td>
        <td><%= popular_video.img %></td>
        <td><%= link_to 'Show', popular_video %></td>
        <td><%= link_to 'Edit', edit_popular_video_path(popular_video) %></td>
        <td><%= link_to 'Destroy', popular_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Popular video', new_popular_video_path %>
