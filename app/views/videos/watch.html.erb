                    <!-- FEATURED BOX -->
                    <div class="span-mainbox-cont">
                        <h3></h3>
                        <div id='playerxOMAufbyCsCE'></div>
                        <script type='text/javascript'>
                        jwplayer('playerxOMAufbyCsCE').setup({
                        file: 'https://www.youtube.com/watch?v=<%= @url %>',
                        image: 'https://s3-eu-west-1.amazonaws.com/afrimages/yt/ws/<%= @img %>',
                        width: '100%',
                        aspectratio: '16:9',
                        autostart: 'true'
                        });
                        </script>
                        <!-- </div> -->
                        <div class="span-post">
                            <!-- CONTENT -->
                            <div class="inner-content" style="margin-top:10px;">
