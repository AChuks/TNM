<h1>Listing trend_fashions</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Mdata</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trend_fashions.each do |trend_fashion| %>
      <tr>
        <td><%= trend_fashion.url %></td>
        <td><%= trend_fashion.mdata %></td>
        <td><%= trend_fashion.img %></td>
        <td><%= link_to 'Show', trend_fashion %></td>
        <td><%= link_to 'Edit', edit_trend_fashion_path(trend_fashion) %></td>
        <td><%= link_to 'Destroy', trend_fashion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trend fashion', new_trend_fashion_path %>
