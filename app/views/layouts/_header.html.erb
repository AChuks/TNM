<div class="span-container">
    <div class="span-29 container-blank">
        <!-- TOP-LEFT MENU -->
        <div class="top_div">
            <ul class="top_div">
                <li><%= link_to "Submit Video", vsubmit_path %></li>
                <li><%= link_to "Contact", contact_path %></li>
                <li><%= link_to "Advertise", advertise_path %></li>
            </ul>
            <!-- STYLE SWITCHER. TOP-RIGHT -->
            <div class="span-11">
                <a class="skin-default-btn"  onclick="chooseStyle('cyan-theme',2060);" ></a>
                <a class="skin-lime-btn" onclick="chooseStyle('lime-theme',2060);" ></a>
                <a class="skin-orange-btn" onclick="chooseStyle('orange-theme',2060);" ></a>
            </div>
        </div>
        <div class="span-28">
            <!-- LOGO -->
            <div class="span-25">
                <a href="/" class="main-logo" title="Tuber - Social Video Theme"></a>
            </div>
        </div>
    </div>
</div>